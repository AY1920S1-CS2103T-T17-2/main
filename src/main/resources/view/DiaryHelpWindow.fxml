<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>

<fx:root resizable="false" title="Help" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/help_icon.png" />
    </icons>
    <scene>
        <Scene>
            <VBox alignment="CENTER" prefHeight="315.0" prefWidth="530.0">
                <children>
               <VBox prefHeight="200.0" prefWidth="100.0">
                  <children>
                     <HBox prefHeight="47.0" prefWidth="515.0">
                        <children>
                                <Label fx:id="helpMessage" prefHeight="17.0" prefWidth="445.0" text="Get Help from the user guide">
                                    <HBox.margin>
                                        <Insets right="5.0" />
                                    </HBox.margin>
                                </Label>
                                <Button fx:id="copyButton" mnemonicParsing="false" onAction="#copyUrl" text="Copy URL">
                                    <HBox.margin>
                                        <Insets left="5.0" />
                                    </HBox.margin>
                                </Button>
                        </children>
                     </HBox>
                     <HBox prefHeight="137.0" prefWidth="515.0">
                        <children>
                           <TitledPane prefHeight="119.0" prefWidth="200.0" text=" add parameters">
                              <content>
                                 <VBox prefHeight="91.0" prefWidth="198.0">
                                    <children>
                                       <HBox prefHeight="23.0" prefWidth="183.0">
                                          <children>
                                             <Label prefHeight="17.0" prefWidth="171.0" text="title,date can't be empty" />
                                          </children>
                                       </HBox>
                                       <HBox prefHeight="22.0" prefWidth="177.0">
                                          <children>
                                             <Label prefHeight="30.0" prefWidth="173.0" text="date: dd/mm/yyyy HHmm" />
                                          </children>
                                       </HBox>
                                       <HBox prefHeight="27.0" prefWidth="177.0">
                                          <children>
                                             <Label prefHeight="25.0" prefWidth="169.0" text="memory,place are option" />
                                          </children>
                                       </HBox>
                                    </children>
                                 </VBox>
                              </content>
                           </TitledPane>
                           <TitledPane prefHeight="120.0" prefWidth="186.0" text="delete, privacy parameters">
                              <content>
                                 <VBox prefHeight="92.0" prefWidth="184.0">
                                    <children>
                                       <HBox prefHeight="17.0" prefWidth="178.0">
                                          <children>
                                             <Label prefHeight="17.0" prefWidth="170.0" text="&quot;x&quot; is a positive integer" />
                                          </children>
                                       </HBox>
                                       <HBox prefHeight="16.0" prefWidth="178.0">
                                          <children>
                                             <Label prefHeight="4.0" prefWidth="169.0" text="password/user: " />
                                          </children>
                                       </HBox>
                                       <HBox prefHeight="17.0" prefWidth="178.0">
                                          <children>
                                             <Label prefHeight="17.0" prefWidth="179.0" text="more than 7 " />
                                          </children>
                                       </HBox>
                                       <HBox prefHeight="100.0" prefWidth="200.0">
                                          <children>
                                             <Label prefHeight="17.0" prefWidth="173.0" text="alphanumeric chars" />
                                          </children>
                                       </HBox>
                                    </children>
                                 </VBox>
                              </content>
                           </TitledPane>
                           <TitledPane prefHeight="200.0" prefWidth="200.0" text="find parameters">
                              <content>
                                 <VBox prefHeight="200.0" prefWidth="100.0">
                                    <children>
                                       <HBox prefHeight="16.0" prefWidth="135.0">
                                          <children>
                                             <Label prefHeight="17.0" prefWidth="128.0" text="&quot;something&quot;: cant be " />
                                          </children>
                                       </HBox>
                                       <HBox prefHeight="13.0" prefWidth="135.0">
                                          <children>
                                             <Label prefHeight="17.0" prefWidth="131.0" text="empty" />
                                          </children>
                                       </HBox>
                                       <HBox prefHeight="23.0" prefWidth="135.0">
                                          <children>
                                             <Label prefHeight="17.0" prefWidth="132.0" text="prefix: t/d/p/m" />
                                          </children>
                                       </HBox>
                                    </children>
                                 </VBox>
                              </content>
                           </TitledPane>
                        </children>
                     </HBox>
                  </children>
               </VBox>
               <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
                 <tabs>
                   <Tab text="Add and delete">
                        <content>
                           <VBox prefHeight="200.0" prefWidth="100.0">
                              <children>
                                 <Label prefHeight="75.0" prefWidth="369.0" text="add command: add t/title d/date [p/place] [m/memory]" />
                                 <Label prefHeight="44.0" prefWidth="367.0" text="delete command: delete x" />
                              </children>
                           </VBox>
                        </content>
                     </Tab>
                   <Tab text="Find, Find Specific and List">
                        <content>
                           <VBox prefHeight="200.0" prefWidth="100.0">
                              <children>
                                 <Label prefHeight="63.0" prefWidth="365.0" text="find command: find something" />
                                 <Label prefHeight="50.0" prefWidth="358.0" text="findSpecific command: findSpecific prefix/something" />
                                 <Label prefHeight="46.0" prefWidth="359.0" text="list command: list" />
                              </children>
                           </VBox>
                        </content>
                     </Tab>
                     <Tab text="Privacy">
                        <content>
                           <VBox prefHeight="200.0" prefWidth="100.0">
                              <children>
                                 <Label prefHeight="62.0" prefWidth="355.0" text="private command: private x" />
                                 <Label prefHeight="17.0" prefWidth="355.0" text="unprivate command: unprivate x" />
                                 <Label prefHeight="50.0" prefWidth="419.0" text="unlock command: unlock x user/username password/password" />
                                 <Label text="setDetails command: setDetails user/username password/password" />
                              </children>
                           </VBox>
                        </content>
                     </Tab>
                 </tabs>
               </TabPane>
                </children>
                <opaqueInsets>
                    <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
                </opaqueInsets>
                <padding>
                    <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
                </padding>
            </VBox>
        </Scene>
    </scene>
</fx:root>
